#include<bits/stdc++.h>
using namespace std;
int node,edge,i,j;
int mat[100][100];   // Or int mat[n][n]

int main(){
    scanf("%d%d", &node, &edge);
    for(i=0; i<edge; i++){
        int u,v,cost;
        scanf("%d%d%d", &u, &v, &cost);
        mat[u][v]=cost;
        mat[v][u]=cost;
    }
    for(i=1; i<=node; i++){
        for(j=1; j<=node; j++){
            cout<<mat[i][j]<<" ";
        }
        cout<<endl;
    }
}
