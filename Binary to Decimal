#include <bits/stdc++.h>
using namespace std;
void ac(){
    int n,s,i=0,rem,dec=0;  cin>>s;
    while(s!=0){
        rem = s%10;
        s/=10;
        dec = dec + (rem*pow(2,i));
        i++;
    }
    cout<<dec<<endl;
}
int main() {
	int tc;     cin>>tc;
	while(tc--){
	    ac();
	}
	return 0;
}
